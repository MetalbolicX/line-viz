{
  "name": "line-viz",
  "version": "0.1.0",
  "description": "A D3.js web component that render a multiseries numerical line.",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "imports": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsdown"
  },
  "author": {
    "name": "José Martínez Santana"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetalbolicX/line-viz.git"
  },
  "bugs": {
    "url": "https://github.com/MetalbolicX/line-viz/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "tsdown": "^0.14.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "tipviz": "^2.0.2"
  },
  "keywords": [
    "d3",
    "line",
    "chart",
    "visualization"
  ]
}
